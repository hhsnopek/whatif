:stylus
  @import url(http://fonts.googleapis.com/css?family=Raleway:300)

  blue = #00A7F5
  red = #d61e1e
  charcoal = #232324

  body
    margin 0
    padding 0
    font-family 'Raleway' sans-serif
    font-weight 300
    background-color charcoal

  header, #headertwo, nav
    text-align center

  header
    color blue
    font-size 24px
    margin-top 17%

  #headertwo
    margin-top 5%
    color red

  nav
    font-size 14px
    list-style none
    margin-top 15%

    li
      display inline
      margin-right 60px

      &:last-child
        margin-right 32px

      h4
        display inline-block
      a
        color blue
        text-decoration none
